{
  "name": "@tailify/eslint-config-legacy",
  "version": "0.0.0",
  "description": "Tailify's very own opinionated ESLint configuration for ES5 projects",
  "main": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": "tailify/eslint-config-legacy",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "license": "UNLICENSED",
  "bugs": "https://github.com/tailify/eslint-config-legacy/issues",
  "homepage": "https://github.com/tailify/eslint-config-legacy#readme",
  "dependencies": {
    "eslint-plugin-node": "8.0.1"
  },
  "devDependencies": {
    "eslint": "^5.9.0"
  },
  "peerDependencies": {
    "eslint": "^5.9.0"
  },
  "engines": {
    "node": ">=8"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "Update ${dependency} to version ${version}",
      "devDependencyUpdate": "Update ${dependency} to version ${version}",
      "dependencyPin": "Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": "Pin ${dependency} to ${oldVersion}"
    },
    "prTitles": {
      "basicPR": "Update ${dependency} to the latest version",
      "groupPR": "Update ${dependency} in group ${group} to the latest version"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
